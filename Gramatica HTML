GRAMÃ�TICA HTML:

	programa : HTML head body HTMLFIN
	
	head : HEAD title link HEADFIN
	
	title : TITLE TEXTO* TITLEFIN
			
	link : LINK atributosLink+ MAYOR
	
	atributosLink : href | rel | type
	
	href : HREF IGUAL CADENA
	
	rel : REL IGUAL CADENA
	
	type : TYPE IGUAL CADENA
	
	body : BODY etiquetasBody* BODYFIN
	
	etiquetasBody : h1 | h2 | p
	
	h1 : H1 atributosBody* H1FIN
	
	h2 : H2 atributosBody* H2FIN
	
	p : P atributosBody* PFIN
	
	atributosBody : TEXTO | negrita | cursiva | underline
		
	negrita : NEGRITA TEXTO* NEGRITAFIN
	
	cursiva : CURSIVA TEXTO* CURSIVAFIN
	
	underline : UNDERLINE TEXTO* UNDERLINEFIN
			
	HTML : '<html>'
	HTMLFIN : '</html>'
	HEAD : '<head>'
	HEADFIN : '</head>'
	TITLE : '<title>'
	TITLEFIN : '</title>'
	LINK : '<link'
	HREF : 'href'
	REL : 'rel'
	TYPE : 'type'
	IGUAL : '='
	BODY : '<body>'
	BODYFIN : '</body>'
	H1 : '<h1>'
	H1FIN : '</h1>'
	H2 : '<h2>'
	H2FIN : '</h2>'
	P : '<p>'
	PFIN : '</p>'
	MAYOR : '>'
	NEGRITA : '<b>'
	NEGRITAFIN : '</b>'
	CURSIVA : '<i>'
	CURSIVAFIN : '</i>'
	UNDERLINE : '<u>'
	UNDERLINEFIN : '</u>'
	CADENA : "[a-zA-Z0-1.,;/]+"
	TEXTO : [a-zA-Z0-1.,;/]*
	
	